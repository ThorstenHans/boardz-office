<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
	<Id>6163aae9-c182-4336-9dba-ce7a6ad95faa</Id>
	<Version>1.1.0.0</Version>
	<ProviderName>Thinktecture AG</ProviderName>
	<DefaultLocale>en-US</DefaultLocale>
	<DisplayName DefaultValue="boardZ" />
	<Description DefaultValue="All about board games" />
	<IconUrl DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt32.png" />
	<HighResolutionIconUrl DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt64.png" />
	<SupportUrl DefaultValue="http://thinktecture.com" />
	<AppDomains>
		<AppDomain>https://boardz-api.azurewebsites.net</AppDomain>
		<AppDomain>https://boardz-app.azurewebsites.net</AppDomain>
		<AppDomain>https://boardz-id.azurewebsites.net</AppDomain>
    <AppDomain>https://github.com/ThorstenHans/boardz</AppDomain>
	</AppDomains>
	<Hosts>
		<Host Name="Workbook" />
	</Hosts>
	<DefaultSettings>
		<SourceLocation DefaultValue="https://boardz-app.azurewebsites.net/index.html" />
	</DefaultSettings>
	<Permissions>ReadWriteDocument</Permissions>
	<VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
		<Hosts>
			<Host xsi:type="Workbook">
				<DesktopFormFactor>
					<GetStarted>
						<Title resid="Boardz.GetStarted.Title" />
						<Description resid="Boardz.GetStarted.Description" />
						<LearnMoreUrl resid="Boardz.GetStarted.LearnMoreUrl" />
					</GetStarted>
					<FunctionFile resid="Boardz.DesktopFunctionFile.Url" />
					<ExtensionPoint xsi:type="PrimaryCommandSurface">
						<CustomTab id="BoardzTab">
							<Group id="Thinktecture.BoardzGroup">
								<Label resid="Boardz.BoardzGroupLabel" />
								<Icon>
									<bt:Image size="16" resid="Boardz.Icon16" />
									<bt:Image size="32" resid="Boardz.Icon32" />
									<bt:Image size="40" resid="Boardz.Icon40" />
									<bt:Image size="48" resid="Boardz.Icon48" />
									<bt:Image size="64" resid="Boardz.Icon64" />
									<bt:Image size="80" resid="Boardz.Icon80" />
								</Icon>
								<Control xsi:type="Button" id="Boardz.OpenBoardzButton">
									<Label resid="Boardz.OpenBoardzButton.Label" />
									<Supertip>
										<Title resid="Boardz.OpenBoardzButton.Label" />
										<Description resid="Boardz.OpenBoardzButton.Tooltip" />
									</Supertip>
									<Icon>
										<bt:Image size="16" resid="Boardz.Icon16" />
										<bt:Image size="20" resid="Boardz.Icon20" />
										<bt:Image size="24" resid="Boardz.Icon24" />
										<bt:Image size="32" resid="Boardz.Icon32" />
										<bt:Image size="40" resid="Boardz.Icon40" />
										<bt:Image size="48" resid="Boardz.Icon48" />
										<bt:Image size="64" resid="Boardz.Icon64" />
										<bt:Image size="80" resid="Boardz.Icon80" />
									</Icon>
									<Action xsi:type="ShowTaskpane">
										<TaskpaneId>OpenBoardzButtonAction</TaskpaneId>
										<SourceLocation resid="Boardz.Taskpane.Url" />
									</Action>
								</Control>
								<Control xsi:type="Button" id="Boardz.OpenBoardzRepo">
									<Label resid="Boardz.OpenBoardzRepo.Label" />
									<Supertip>
										<Title resid="Boardz.OpenBoardzRepo.Label" />
										<Description resid="Boardz.OpenBoardzRepo.Tooltip" />
									</Supertip>
									<Icon>
										<bt:Image size="16" resid="Boardz.gh16" />
										<bt:Image size="20" resid="Boardz.gh20" />
										<bt:Image size="24" resid="Boardz.gh24" />
										<bt:Image size="32" resid="Boardz.gh32" />
										<bt:Image size="40" resid="Boardz.gh40" />
										<bt:Image size="48" resid="Boardz.gh48" />
										<bt:Image size="64" resid="Boardz.gh64" />
										<bt:Image size="80" resid="Boardz.gh80" />
									</Icon>
									<Action xsi:type="ExecuteFunction">
										<FunctionName>openRepository</FunctionName>
									</Action>
								</Control>
							</Group>
							<Label resid="Boardz.BoardzTab.Label" />
						</CustomTab>
					</ExtensionPoint>
				</DesktopFormFactor>
			</Host>
		</Hosts>
		<Resources>
			<bt:Images>
				<bt:Image id="Boardz.Icon16" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt16.png" />
				<bt:Image id="Boardz.Icon20" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt20.png" />
				<bt:Image id="Boardz.Icon24" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt24.png" />
				<bt:Image id="Boardz.Icon32" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt32.png" />
				<bt:Image id="Boardz.Icon40" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt40.png" />
				<bt:Image id="Boardz.Icon48" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt48.png" />
				<bt:Image id="Boardz.Icon64" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt64.png" />
				<bt:Image id="Boardz.Icon80" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/tt80.png" />
				<bt:Image id="Boardz.gh16" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh16.png" />
				<bt:Image id="Boardz.gh20" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh20.png" />
				<bt:Image id="Boardz.gh24" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh24.png" />
				<bt:Image id="Boardz.gh32" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh32.png" />
				<bt:Image id="Boardz.gh40" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh40.png" />
				<bt:Image id="Boardz.gh48" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh48.png" />
				<bt:Image id="Boardz.gh64" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh64.png" />
				<bt:Image id="Boardz.gh80" DefaultValue="https://boardz-app.azurewebsites.net/assets/images/gh80.png" />
			</bt:Images>
			<bt:Urls>
				<bt:Url id="Boardz.DesktopFunctionFile.Url" DefaultValue="https://boardz-app.azurewebsites.net/functions.html" />
				<bt:Url id="Boardz.Taskpane.Url" DefaultValue="https://boardz-app.azurewebsites.net/index.html" />
				<bt:Url id="Boardz.GetStarted.LearnMoreUrl" DefaultValue="http://thinktecture.com" />
			</bt:Urls>
			<bt:ShortStrings>
				<bt:String id="Boardz.BoardzTab.Label" DefaultValue="Thinktecture" />
				<bt:String id="Boardz.OpenBoardzButton.Label" DefaultValue="boardZ" />
				<bt:String id="Boardz.OpenBoardzRepo.Label" DefaultValue="Repository" />
				<bt:String id="Boardz.BoardzGroupLabel" DefaultValue="boardZ" />
				<bt:String id="Boardz.GetStarted.Title" DefaultValue="Hey, boardZ is here." />
			</bt:ShortStrings>
			<bt:LongStrings>
				<bt:String id="Boardz.OpenBoardzButton.Tooltip" DefaultValue="Click to start boardZ in Excel" />
				<bt:String id="Boardz.OpenBoardzRepo.Tooltip" DefaultValue="Click to open boardZ Repo" />
				<bt:String id="Boardz.GetStarted.Description" DefaultValue="Access all your boardgames directly from Excel" />
			</bt:LongStrings>
		</Resources>
	</VersionOverrides>
</OfficeApp>
